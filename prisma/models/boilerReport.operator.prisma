model Operator {
  id                      String                  @id @default(cuid())
  boilerReportId          String                  @unique
  name                    String
  isAbleToOperateWithNR13 String
  certificateId           String?                 @unique
  provisionsForOperator   String?
  observations            String?
  boilerReport            BoilerReport            @relation(fields: [boilerReportId], references: [id])
  certificate             BoilerReportAttachment? @relation(fields: [certificateId], references: [id])
}
