model Storage {
  id             String        @id @default(cuid())
  companyId      String
  relativeLink   String
  status         StorageStatus @default(ACTIVE)
  createdAt      DateTime      @default(now())
  updatedAt      DateTime      @updatedAt
  company        Company       @relation(fields: [companyId], references: [id])
  storageUnits   StorageUnit[]
}
