model BoilerInfo {
  id                       String         @id @default(cuid())
  manufacturer             String?
  mark                     String?
  type                     BoilerType
  model                    String?
  yearOfManufacture        Int?
  maximumWorkingPressure   String?
  maximumOperatingPressure String?
  series                   String?
  fuelType                 BoilerFuelType
  category                 BoilerCategory
  boilerReportId           String         @unique
  boilerReport             BoilerReport   @relation(fields: [boilerReportId], references: [id])
}
