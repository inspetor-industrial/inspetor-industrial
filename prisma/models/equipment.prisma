model Equipment {
  id String @id @default(cuid())

  company   Company @relation(fields: [companyId], references: [id])
  companyId String

  name String
  mark String

  identificationNumber String @unique

  type String

  manufactorYear String @db.VarChar(10)
  model          String
  category       String
  pmta           String

  createdAt        DateTime           @default(now())
  updatedAt        DateTime           @updatedAt
  DailyMaintenance DailyMaintenance[]

  @@index([companyId])
  @@index([id, companyId])
}
