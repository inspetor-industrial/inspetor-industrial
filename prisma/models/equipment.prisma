model Equipment {
  id                   String             @id @default(cuid())
  companyId            String
  name                 String
  model                String
  createdAt            DateTime           @default(now())
  updatedAt            DateTime           @updatedAt
  category             String
  identificationNumber String             @unique
  manufactorYear       String             @db.VarChar(10)
  mark                 String
  pmta                 String
  type                 String
  DailyMaintenance     DailyMaintenance[]
  company              Company            @relation(fields: [companyId], references: [id])

  @@index([companyId])
  @@index([id, companyId])
}
