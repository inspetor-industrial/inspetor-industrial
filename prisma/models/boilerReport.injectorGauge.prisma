model InjectorGauge {
  id String @id @default(cuid())

  boilerReport   BoilerReport @relation(fields: [boilerReportId], references: [id])
  boilerReportId String       @unique

  tests Json

  observations String?

  fuelType InjectorGaugeFuel

  mark         String
  diameter     String
  serialNumber String @map("serial_number")

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  photos BoilerReportAttachment[]
}
